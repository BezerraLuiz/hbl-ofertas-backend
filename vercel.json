{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node",
      "config": {
        "env": {
          "PRISMA_GENERATE": "true"
        }
      }
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE": "true"
    }
  },
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/app.ts"
    }
  ],
  "functions": {
    "api/**/*": {
      "maxDuration": 20
    }
  }
}
